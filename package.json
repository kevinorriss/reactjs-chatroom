{
  "name": "reactjs-chatroom",
  "version": "1.0.0",
  "scripts": {
    "component": "yarn --cwd component run dev",
    "socket": "yarn --cwd socket run dev",
    "server": "yarn --cwd server start",
    "client": "yarn --cwd client start",
    "dev": "concurrently \"yarn nodemon socket\" \"yarn run server\" \"yarn run component\" \"yarn run client\""
  },
  "author": "Kevin Orriss <kevinorriss@gmail.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
